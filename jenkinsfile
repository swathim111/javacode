node {
stage('git checkout') {
git branch: 'main', credentialsId: 'git', url: 'https://github.com/swathim111/javacode.git' 
}
stage('test') {
sh 'mvn test'
}
stage('clean') {
sh 'mvn clean install'
}
stage('code analysis') {
withSonarQubeEnv(credentialsId: 'sonarid') {
sh "mvn clean package sonar:sonar"
}
}

}
